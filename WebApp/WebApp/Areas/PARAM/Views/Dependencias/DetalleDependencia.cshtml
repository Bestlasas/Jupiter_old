@model DetalleDependencia
@{
    var viewName = System.IO.Path.GetFileNameWithoutExtension(Path);
}

<div class="form-group row">
    <div class="col-sm-12">
        <h5>Codigor de la dependencia</h5>
        <div>@Model.Codigo</div>
    </div>
    
</div>
<div class="form-group row">
    <div class="col-sm-12">
        <h5>Descripcion de la dependencia</h5>
        <div>@Model.Descripcion</div>
    </div>

</div>

<div class="form-group row">
    <div class="col-sm-12">
        <h5>Colaborador lider de la dependencia</h5>
        <div>@Model.Colaborador</div>
    </div>

</div>


